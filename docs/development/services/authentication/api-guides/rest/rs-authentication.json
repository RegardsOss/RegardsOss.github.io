    "/oauth/token": {
      "post": {
        "operationId": "postAccessToken",
        "requestBody": {
          "content": {
            "*/*": {
              "schema": {
                "$ref": "#/components/schemas/UserAuthentication"
              }
            }
          },
          "description": "Authentication credentials."
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Authentication"
                }
              }
            },
            "description": "Authentication granted."
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Authentication"
                }
              }
            },
            "description": "Authentication denied."
          }
        },
        "summary": "Oauth2 authentication.",
        "tags": [
          "token-controller"
        ]
      }
    },
