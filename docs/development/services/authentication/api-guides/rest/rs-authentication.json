  "paths": {
    "/borrowRole/{target_name}": {
      "get": {
        "operationId": "switchRole",
        "parameters": [
          {
            "in": "path",
            "name": "target_name",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/Authentication"
                }
              }
            },
            "description": "Role switched successfully."
          },
          "403": {
            "content": {
              "application/html": {}
            },
            "description": "The endpoint is not accessible for the user."
          }
        },
        "summary": "Switch current user role.",
        "tags": [
          "borrow-role-controller"
        ]
      }
    },
    "/maintenance": {
      "get": {
        "operationId": "retrieveTenantsInMaintenance",
